{
  "rules": {
    "no-use-before-define": [2, "nofunc"],
    "no-lonely-if": 2,
    "no-else-return": 2,
    "no-new": 2,
    "no-throw-literal": 2,
    "no-self-compare": 2,
    "no-void": 2,
    "no-eq-null": 2,

    "quotes": [2, "single"],
    "indent": 2,
    "camelcase": 2,
    "comma-style": 2,
    "key-spacing": 2,
    "new-cap": 2,
    "no-empty": 2,
    "no-multi-spaces": 2,
    "brace-style": 2,
    "wrap-iife": 2,

    "space-after-function-name": 2,
    "space-in-parens": 2,
    "space-before-blocks": 2,
    "space-after-keywords": 2,
    "space-unary-ops": 2,
    "space-before-function-parentheses": [2, {"anonymous": "always", "named": "never"}],
    "space-in-brackets": 2,

    "curly": 0,
    "no-underscore-dangle": 0,
    "no-constant-condition": 0,
    "no-shadow": 0,
    "comma-spacing": 0
  },
  "env": {
    "node": true,
    "browser": true
  }
}
